{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "indent": "off",
        "no-undef": "off",
        "prefer-const": "off",
        "no-unused-vars": "warn",
        "space-before-function-paren": "off",
        "vue/multi-word-component-names": "off",
        "vue/no-unused-components": "warn"
    }
}